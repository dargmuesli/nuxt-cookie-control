{
  "name": "@dargmuesli/nuxt-cookie-control",
  "version": "6.4.1",
  "description": "Nuxt Cookie Control Module",
  "author": "Dario Ferderber <dario.ferderber@broj42.com>",
  "maintainers": [
    "Jonas Thelemann"
  ],
  "funding": {
    "url": "https://github.com/sponsors/dargmuesli"
  },
  "keywords": [
    "nuxt cookie control",
    "nuxt gdpr",
    "nuxt cookie consent",
    "nuxt gdpr consent"
  ],
  "repository": "https://github.com/dargmuesli/nuxt-cookie-control",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.6.12",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist/**/*",
    "src/components",
    "src/locale"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "lint": "eslint --ext .js,.ts,.vue . && nuxt typecheck",
    "lint:fix": "eslint --ext .js,.ts,.vue --fix . && nuxt typecheck --fix",
    "prepack": "pnpm build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxt/kit": "3.7.0",
    "@sindresorhus/slugify": "2.2.1",
    "css-vars-ponyfill": "2.4.8",
    "string-replace-loader": "3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@dargmuesli/nuxt-cookie-control": "link:.",
    "@nuxt/module-builder": "0.4.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "nuxt": "3.7.0",
    "prettier": "3.0.2",
    "typescript": "5.2.2",
    "vue": "3.3.4",
    "vue-tsc": "1.8.8",
    "webpack": "5.88.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
