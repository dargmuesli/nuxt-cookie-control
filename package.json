{
  "name": "@dargmuesli/nuxt-cookie-control",
  "version": "7.0.0-beta.2",
  "description": "Nuxt Cookie Control Module",
  "author": "Dario Ferderber <dario.ferderber@broj42.com>",
  "maintainers": [
    "Jonas Thelemann"
  ],
  "funding": {
    "url": "https://github.com/sponsors/dargmuesli"
  },
  "keywords": [
    "nuxt cookie control",
    "nuxt gdpr",
    "nuxt cookie consent",
    "nuxt gdpr consent"
  ],
  "repository": "https://github.com/dargmuesli/nuxt-cookie-control",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.9.2",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist/**/*",
    "src/components",
    "src/locale"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "lint": "eslint --ext .js,.ts,.vue . && nuxt typecheck",
    "lint:fix": "eslint --ext .js,.ts,.vue --fix . && nuxt typecheck --fix",
    "prepack": "pnpm build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.0",
    "@sindresorhus/slugify": "^2.2.1",
    "css-vars-ponyfill": "^2.4.8",
    "string-replace-loader": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.0.0",
    "@commitlint/config-conventional": "18.0.0",
    "@dargmuesli/nuxt-cookie-control": "link:",
    "@nuxt/module-builder": "0.5.2",
    "@nuxt/schema": "3.8.0",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "nuxt": "3.8.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "vue": "3.3.6",
    "vue-tsc": "1.8.19",
    "webpack": "5.89.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
