{
  "author": "Jonas Thelemann",
  "contributors": [
    "Dario Ferderber <dario.ferderber@broj42.com>"
  ],
  "dependencies": {
    "@nuxt/kit": "^3.8.0",
    "css-vars-ponyfill": "^2.4.8",
    "string-replace-loader": "^3.1.0"
  },
  "description": "Nuxt Cookie Control Module",
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@dargmuesli/nuxt-cookie-control": "link:",
    "@nuxt/eslint-config": "0.3.10",
    "@nuxt/module-builder": "0.6.0",
    "@nuxt/schema": "3.11.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "12.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "10.0.3",
    "@semantic-release/npm": "12.0.0",
    "@semantic-release/release-notes-generator": "13.0.0",
    "eslint": "9.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "nuxt": "3.11.2",
    "prettier": "3.2.5",
    "semantic-release": "23.0.8",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vue": "3.4.27",
    "vue-tsc": "2.0.16",
    "webpack": "5.91.0"
  },
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    },
    "./runtime/*": {
      "import": "./dist/runtime/*.mjs",
      "require": "./dist/runtime/*.mjs",
      "types": "./dist/runtime/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "funding": {
    "url": "https://github.com/sponsors/dargmuesli"
  },
  "keywords": [
    "nuxt cookie control",
    "nuxt gdpr",
    "nuxt cookie consent",
    "nuxt gdpr consent"
  ],
  "license": "MIT",
  "main": "./dist/module.cjs",
  "maintainers": [
    "Jonas Thelemann"
  ],
  "name": "@dargmuesli/nuxt-cookie-control",
  "packageManager": "pnpm@9.1.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/dargmuesli/nuxt-cookie-control",
  "resolutions": {
    "@nuxt/kit": "3.11.2"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "pnpm --dir playground run dev",
    "lint": "pnpm run lint:js && pnpm run lint:ts",
    "lint:fix": "pnpm run lint:js --fix . && pnpm run lint:ts --fix",
    "lint:js": "eslint --cache",
    "lint:ts": "vue-tsc --noEmit",
    "prepack": "pnpm build",
    "prepare": "husky && nuxt-module-build prepare"
  },
  "type": "module",
  "types": "./dist/types.d.ts",
  "version": "8.2.0"
}
